public static {{className.pascalCase}} Dummy{{className.pascalCase}}() {

    {{className.pascalCase}} dummy = new {{className.pascalCase}}();

{{#if supertype}}
    {{supertype.className.pascalCase}} supertype = Dummy{{supertype.className.pascalCase}}();

    {{#each supertype.fields}}
